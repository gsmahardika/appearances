{
    "name": "wiratama/appearances",
    "type": "project",
    "description": "Laravel 8 theming package, based on floatingpoint/stylist",
    "authors": [
        {
            "name": "Arya Wiratama",
            "email": "aryawiratama99@gmail.com"
        }
    ],
    "keywords": [
        "theme",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": ">=8.0",
        "laravelcollective/html": "~6.0",
        "laravel/framework": "~8.0"
    },
    "require-dev": {
        "mockery/mockery": "dev-master@dev"
    },
    "autoload": {
        "psr-4": {
            "Wiratama\\Appearances\\": "src/Appearances"
        }
    },
    "extra": {
        "laravel": {
            "providers": "Wiratama\\Appearances\\AppearancesServiceProvider"
        }
    },
    "minimum-stability": "dev"
}
