{
    "name": "gsmahardika/appearances",
    "type": "project",
    "description": "Laravel 10 theming package, based on floatingpoint/stylist",
    "authors": [
        {
            "name": "GS Mahardika",
            "email": "gs.mahardika@gmail.com"
        }
    ],
    "keywords": [
